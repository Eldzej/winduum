{
  "name": "winduum",
  "version": "0.1.6",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build-prod": "vite build -c vite.config.prod.js",
    "eslint": "eslint '**/*.js' --fix",
    "stylelint": "npx stylelint '**/*.css' --fix",
    "tailwind": "rollup utils/tailwind.js --file utils/tailwind.cjs -f cjs --exports named"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "css-has-pseudo": "^5.0.1",
    "eslint": "^8.33.0",
    "eslint-config-standard": "^17.0.0",
    "postcss": "^8.4.21",
    "postcss-custom-media": "^9.1.1",
    "postcss-custom-properties": "^13.1.1",
    "postcss-custom-selectors": "^7.1.1",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^11.1.0",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^3.2.4",
    "fast-glob": "^3.2.12",
    "vite": "^4.1.1",
    "vue": "^3.2.47"
  },
  "files": [
    "index.js",
    "tailwind.config.cjs",
    "dist",
    "src",
    "utils"
  ],
  "exports": {
    ".": "./utils/tailwind.cjs",
    "./tailwind-cjs": "./utils/tailwind.cjs",
    "./tailwind-esm": "./utils/tailwind.js",
    "./main.css": "./dist/main.css",
    "./tailwind.css": "./dist/tailwind.css",
    "./src/*": "./src/*",
    "./utils/*": "./utils/*"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winduum/winduum.git"
  }
}
