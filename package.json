{
  "name": "winduum",
  "version": "0.2.0",
  "type": "module",
  "main": "utils/tailwind.cjs",
  "module": "utils/tailwind.js",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build-prod": "vite build -c vite.config.prod.js",
    "eslint": "eslint 'src/**/*.js' --fix",
    "stylelint": "npx stylelint 'src/**/*.css' --fix",
    "tailwind": "rollup utils/tailwind.js --file utils/tailwind.cjs -f cjs --exports named"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "css-has-pseudo": "^5.0.2",
    "eslint": "^8.42.0",
    "eslint-config-standard": "^17.1.0",
    "postcss": "^8.4.24",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^11.2.2",
    "postcss-custom-media": "^9.1.4",
    "stylelint-config-standard": "33.0.0",
    "stylelint-stylistic": "^0.4.2",
    "tailwindcss": "^3.3.2",
    "fast-glob": "^3.2.12",
    "vite": "^4.3.9",
    "vue": "^3.3.4"
  },
  "files": [
    "index.js",
    "tailwind.config.cjs",
    "dist",
    "src",
    "utils"
  ],
  "exports": {
    "./main.css": "./dist/main.css",
    "./tailwind.css": "./dist/tailwind.css",
    "./src/*": "./src/*",
    "./utils/*": "./utils/*"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winduum/winduum.git"
  }
}
